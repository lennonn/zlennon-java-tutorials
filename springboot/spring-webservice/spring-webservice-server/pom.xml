					<id>xjc</id>
					<goals>
						<goal>xjc</goal>
					</goals>
				</execution>
			</executions>
			<configuration>
				<sources>
					<source>${project.basedir}/src/main/resources/countries.xsd</source>
				</sources>
				<packageName>com.zlennon.springwebserviceserver.client</packageName>
			</configuration>
			</plugin>
